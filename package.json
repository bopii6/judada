{
  "name": "judada-monorepo",
  "private": true,
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "db:generate": "pnpm --filter server prisma:generate",
    "db:migrate": "pnpm --filter server prisma:migrate",
    "db:deploy": "pnpm --filter server prisma:deploy"
  },
  "devDependencies": {
    "@prisma/client": "5.22.0",
    "prisma": "5.22.0",
    "turbo": "^1.10.14",
    "typescript": "^5.3.3"
  }
}
