{
  "name": "server",
  "private": true,
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "start": "node dist/index.js",
    "worker": "tsx src/workers/packageGeneration.worker.ts",
    "worker:dev": "tsx watch src/workers/packageGeneration.worker.ts",
    "worker:start": "node dist/workers/packageGeneration.worker.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "prisma:generate": "pnpm exec prisma generate --schema \"../../prisma/schema.prisma\"",
    "prisma:migrate": "pnpm exec prisma migrate dev --schema \"../../prisma/schema.prisma\"",
    "prisma:deploy": "pnpm exec prisma migrate deploy --schema \"../../prisma/schema.prisma\""
  },
  "dependencies": {
    "@alicloud/ocr-api20210707": "^3.1.3",
    "@alicloud/openapi-core": "^1.0.5",
    "@alicloud/tea-util": "^1.4.9",
    "@prisma/client": "^5.8.1",
    "@supabase/supabase-js": "^2.43.4",
    "bullmq": "^5.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.4.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "openai": "^4.26.0",
    "papaparse": "^5.4.1",
    "pdf-parse": "^1.1.1",
    "tencentcloud-sdk-nodejs": "^4.1.136",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.1.1",
    "prisma": "^5.22.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
